AS=TASM
ASFLAGS=/c /z /zi /la

LINK=TLINK
LINKFLAGS=/Tde /3 /v

ALL: STUDENT.EXE
	echo " BUILDING FINAL TARGET... "

STUDENT.EXE: MAIN.OBJ SUBMOD1.OBJ SUBMOD2.OBJ SUBMOD3.OBJ COMMOD.OBJ
	$(LINK) $(LINKFLAGS) MAIN.OBJ+SUBMOD1.OBJ+SUBMOD2.OBJ+SUBMOD3.OBJ+SUBMOD4.OBJ+COMMOD.OBJ

.ASM.OBJ:
	$(AS) $(ASFLAGS) *.ASM
